{
  "name": "weatherx",
  "version": "0.0.1",
  "description": "WeatherX with Server-Side-Rendering, using tomtom, forecast.io api",
  "proxy": "http://localhost:3000",
  "main": "webpack.config.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "start": "webpack --watch --progress & nodemon --inspect ./index.js",
    "linter": "eslint src",
    "bundlesize": "bundlesize",
    "precommit": "eslint src --quiet"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.16.3",
    "helmet": "^3.18.0",
    "ignore-styles": "5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sequelize": "^5.8.7",
    "sequelize-cli": "5.4.0",
    "sqlite3": "^4.0.8",
    "throng": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.17.2",
    "css-loader": "2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "5.16.0",
    "eslint-config-fortech-react": "^1.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "7.13.0",
    "express": "^4.17.1",
    "file-loader": "4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "0.7.0",
    "morgan": "1.9.1",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "sass-loader": "7.1.0",
    "webpack": "4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "^3.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "bundlesize": [
    {
      "path": "./dist/js/app.bundle.js",
      "maxSize": "170 kB"
    },
    {
      "path": "./dist/js/lib.bundle.js",
      "maxSize": "110 kB"
    }
  ],
  "jest": {
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-config/test-setup.js"
  },
  "author": "Daniel A. Perez",
  "license": "ISC"
}
