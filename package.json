{
  "name": "weatherx",
  "version": "0.0.3",
  "description": "WeatherX with Server-Side-Rendering, using tomtom, forecast.io api",
  "main": "webpack.config.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "dev-reactApp": "webpack --watch --progress & nodemon --inspect=127.0.0.1:9230 ./index.js",
    "dev-server": "nodemon --inspect=127.0.0.1:9229 backendServer/server.js",
    "linter": "eslint src",
    "bundlesize": "bundlesize",
    "precommit": "eslint src --quiet"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "helmet": "^3.16.0",
    "ignore-styles": "5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sequelize": "5.7.1",
    "sqlite3": "^4.0.6",
    "throng": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "bundlesize": "^0.17.1",
    "css-loader": "2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "5.16.0",
    "eslint-config-fortech-react": "^1.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "file-loader": "3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "0.6.0",
    "morgan": "1.9.1",
    "node-sass": "4.11.0",
    "nodemon": "1.18.11",
    "sass-loader": "7.1.0",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "bundlesize": [
    {
      "path": "./dist/js/app.bundle.js",
      "maxSize": "170 kB"
    },
    {
      "path": "./dist/js/lib.bundle.js",
      "maxSize": "110 kB"
    }
  ],
  "jest": {
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-config/test-setup.js"
  },
  "author": "Daniel A. Perez",
  "license": "ISC"
}
